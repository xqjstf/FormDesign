@model  IList<string>
@{
    ViewBag.Title = "表单字段克隆维护";
    FormConfig fConfig = ViewBag.FormConfig;
}
<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
    <h2 class="sub-header">@ViewBag.Title</h2>
    <form class="form-horizontal" method="post">
        <div class="form-group">
            <label for="inputEmail3" class="col-sm-2 control-label">表单名称</label>
            <div class="col-sm-10">
                @fConfig.Name 
            </div>
        </div>
        <div class="form-group">
            <label for="inputEmail3" class="col-sm-2 control-label">涉及数据表</label>
            <div class="col-sm-10">
                @fConfig.TableName
            </div>
        </div>
        <div class="form-group">
            <label for="inputEmail3" class="col-sm-2 control-label">表单字段</label>
            <div class="col-sm-10">
                <select name="TableName" required class="form-control">
                    <option value=""></option>
                    @foreach (var str in Model)
                    {
                        <option value="@str">@str</option>
                    }
                </select>
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10"> 
                @Html.Hidden("FormId", fConfig.Id)
                <button type="submit" class="btn btn-success">保存</button>
                &nbsp;  &nbsp;  &nbsp;  &nbsp;  &nbsp;
                <a href="@Url.Action("FormConfig")" class="btn btn-info">返回</a>
            </div>
        </div>
    </form>
</div> 